<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.mldn.dibmp.dao.IDistributionDetailsDAO">
	<select id="findByDsid" parameterType="Long" resultType="Distribution">
		SELECT dsid,title,pid,cid,gnum,price,status,note
		FROM  distribution   WHERE  dsid =#{dsid} AND status=0;
	</select>
	
</mapper>